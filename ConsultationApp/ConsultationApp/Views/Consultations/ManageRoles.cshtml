@model IEnumerable<Microsoft.AspNet.Identity.EntityFramework.IdentityRole>
@{
    ViewBag.Title = "Roles";
}

<h2>Roles Listing </h2>

@Html.ActionLink("Create New Role", "New_Role")
<hr />
<div>
    @foreach (var role in Model)
    {
        <p>
            <strong>@role.Name | </strong>
            <span onclick="return confirm('Are you sure to delete?')"><a href="/Consultations/DeleteRole?RoleName=@role.Name" class="delLink" style="color:red;">Delete</a></span> 
        </p>
    }
</div>